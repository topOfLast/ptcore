{
	"name": "ptcore",
	"version": "0.0.1",
	"license": "MIT",
	"bin": "dist/cli.js",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "tsc",
		"start": "tsc --watch",
		"run": "npm run build && dist/cli.js",
		"pretest": "npm run build",
		"test": "xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@babel/core": "7.15.4",
		"git-clone": "^0.2.0",
		"ink": "3.0.9",
		"meow": "9.0.0",
		"react": "17.0.2",
		"rimraf": "^4.4.1"
	},
	"devDependencies": {
		"@ava/typescript": "^2.0.0",
		"@sindresorhus/tsconfig": "^1.0.2",
		"@types/react": "17.0.19",
		"ava": "3.15.0",
		"chalk": "4.1.2",
		"eslint-config-xo-react": "0.25.0",
		"eslint-plugin-react": "7.24.0",
		"eslint-plugin-react-hooks": "4.2.0",
		"ink-testing-library": "2.1.0",
		"typescript": "4.3.5",
		"xo": "0.39.1"
	},
	"ava": {
		"typescript": {
			"extensions": [
				"tsx"
			],
			"rewritePaths": {
				"source/": "dist/"
			}
		}
	},
	"xo": {
		"extends": "xo-react",
		"rules": {
			"react/prop-types": "off"
		}
	}
}
